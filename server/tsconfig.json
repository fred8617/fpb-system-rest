{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext","ESNext.Array"],
    "module": "commonjs",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "target": "ESNext",
    "skipLibCheck": true,
    "sourceMap": true,
    "outDir": "./dist",
    "incremental": true,
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "../",
  },
  "exclude": ["dist", "react-app","asset","node_modules"],
}
